{
    "name": "danieleambrosino/firebase-authentication-bundle",
    "description": "A lightweight, self-contained Symfony bundle providing authentication with JWTs generated from Firebase client SDK.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DanieleAmbrosino\\FirebaseAuthenticationBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Daniele Ambrosino",
            "email": "mail@danieleambrosino.it"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-openssl": "*",
        "psr/cache": ">=3",
        "symfony/http-client": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/dependency-injection": "^7.0.1",
        "symfony/config": "^7.0"
    }
}
